package model.dao;

import java.util.Date;
import java.util.List;

import entities.model.Carro;
import entities.model.Pessoa;



public interface PessoaDao {
	
	 public void inserir (String nome,Date nascimento,String endereco,Carro car,String cpf);
	 public void delete(int id);
	 public void update(String nome,Date nascimento,String endereco,Carro car,String cpf);
	 public Pessoa findById(int id);
	 public List<Pessoa> findAll ();
	 

}
